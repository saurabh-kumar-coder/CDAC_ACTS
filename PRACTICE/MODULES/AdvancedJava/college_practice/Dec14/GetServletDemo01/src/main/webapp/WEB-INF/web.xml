<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  <display-name>GetServletDemo01</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>GetHomePageServlet</servlet-name>
  	<servlet-class>com.saurabh.servlets.GetHomePageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>GetHomePageServlet</servlet-name>
  	<url-pattern>/GetHomePage</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>GetContactPageServlet</servlet-name>
  	<servlet-class>com.saurabh.servlets.GetContactPageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>GetContactPageServlet</servlet-name>
  	<url-pattern>/GetContactUsPage</url-pattern>
  </servlet-mapping>
</web-app>